# This file is managed by Puppet. DO NOT EDIT.

Storage {
  Name = "<%= @name %>"
<% if @device != '' -%>
  Device = <%= @device %>
<% end -%>
<% if @media_type != '' -%>
  Media Type = <%= @media_type %>
<% end -%>
<% if @address != '' -%>
  Address = <%= @address %>
<% end -%>
  SDPort = <%= @sd_port %>
<% if @real_password != '' -%>
  Password = "<%= @real_password %>"
<% end -%>
<% if @max_concurrent != '' -%>
  Maximum Concurrent Jobs = <%= @max_concurrent %>
<% end -%>
  AllowCompression = <%= @allow_compression %>
}
